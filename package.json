{
  "name": "anovalabs",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "server": "nodemon index.js",
    "test": "(dropdb --if-exists test-anovalabs-db && createdb test-anovalabs-db) && mocha --exit",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "engines": {
    "node": "8.12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ANovaBerkeley/anovalabs.git"
  },
  "keywords": [
    "EdTech",
    "Mentorship",
    "Computer",
    "Science",
    "Education"
  ],
  "author": "Christopher Franco Monterrosa",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ANovaBerkeley/anovalabs/issues"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^2.1.1",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.20.13",
    "morgan": "~1.9.0",
    "node-gyp": "^6.1.0",
    "pg": "^7.18.2",
    "request": "^2.88.0",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
